<h1 style="margin-top: 100px;">Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>


<table class="blogtable">
  <thead>
  <tr>
    <th style="width: 4%;">ID</th>
    <th style="width: 20%;">标题</th>
    <th style="width: 50%;">内容</th>
  </tr>
  </thead>
  <tbody>
  <% @photo.each do |photo| %>
      <tr>
        <td style="width: 4%;"><%= photo.id %></td>
        <td style="width: 20%;"><%= photo.title %></td>
        <td style="width: 50%;"><%= photo.path %></td>
      </tr>
  <% end %>
  </tbody>
</table>


